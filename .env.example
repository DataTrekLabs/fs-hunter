# fs-hunter configuration
# Copy to .env and edit as needed. CLI flags override these values.

# Output directory (default: ~)
# FS_HUNTER_OUTPUT_DIR=/sf/home/bthanujan.mahendran/output

# Enable MD5 hash computation (default: true). Set to false to skip hashing.
# ENABLE_HASH=true

# scan defaults (CLI flags override these)
# FS_HUNTER_SCAN_DIRS=scan_dirs.txt
# FS_HUNTER_SCAN_FILES=scan_files.txt

# delta defaults
# FS_HUNTER_DELTA_CSV=delta_manifest.csv

# compare defaults
# FS_HUNTER_COMPARE_SOURCE_PREFIX=/data/v1
# FS_HUNTER_COMPARE_TARGET_PREFIX=/data/v2
# FS_HUNTER_COMPARE_SUBDIRS=compare_subdirs.txt
# FS_HUNTER_COMPARE_FILES=compare_files.txt
